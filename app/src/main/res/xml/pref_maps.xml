<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">


    <Preference
        android:defaultValue="null"
        android:key="@string/pref_profile_picture"
        android:title="@string/pref_profile_picture_txt"
        android:widgetLayout="@layout/pref_picture_select"/>

    <CheckBoxPreference
        android:defaultValue="true"
        android:key="@string/settings_show_countries_key"
        android:title="@string/settings_show_countries_label"
        />

    <CheckBoxPreference
        android:defaultValue="false"
        android:key="@string/settings_show_cities_key"
        android:title="@string/settings_show_cities_label"
        />




</PreferenceScreen>